                            </filterbar:FilterItem>
                        </filterbar:filterItems>
                    </filterbar:FilterBar>
                </f:content>
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <VBox fitContainer="true">
                <items>
                    <Table width="100%" growing="true" growingScrollToLoad="true" growingThreshold="25" mode="SingleSelectLeft" >
                        <headerToolbar>
                            <OverflowToolbar design="Transparent">
                                <content>
                                    <Title textAlign="Begin" text="{i18n>qaCount}"/>
                                    <ToolbarSpacer/>
                                    <Button text="{i18n>delete}" iconFirst="false" type="Transparent">
                                        <layoutData>
                                            <OverflowToolbarLayoutData moveToOverflow="true"/>
                                        </layoutData>
                                    </Button>
                                    <Button text="" icon="sap-icon://add" iconFirst="true" type="Transparent">
                                        <layoutData>
                                            <OverflowToolbarLayoutData moveToOverflow="true"/>
                                        </layoutData>
                                    </Button>
                                    <Button icon="sap-icon://action-settings" type="Transparent">

                                            <OverflowToolbarLayoutData moveToOverflow="true"/>
                                        </layoutData>
                                    </Button>
                                </content>
                            </OverflowToolbar>
                        </headerToolbar>
                        <columns>
                            <Column width="auto" minScreenWidth="Phone">
                                <header>
                                    <Text text="{i18n>developer}" width="auto"/>
                                </header>
                            </Column>
                            <Column width="auto" minScreenWidth="Tablet" demandPopin="true">
                                <header>
                                    <Text text="{i18n>qaDate}" width="auto"/>
                                </header>
                            </Column>
                            <Column width="auto" minScreenWidth="Tablet" demandPopin="true">
                                <header>
                                    <Text text="{i18n>task}" width="auto"/>
                                </header>
                            </Column>
                            <Column width="auto" minScreenWidth="Tablet" demandPopin="true">
                                <header>
                                    <Text text="{i18n>performance}" width="auto"/>
                                </header>
                            </Column>
                            <Column width="auto" minScreenWidth="Phone">
                                <header>
                                    <Text text="{i18n>qa}" width="auto"/>
                                </header>
                            </Column>
                        </columns>
                        <items>
                            <ColumnListItem type="Active">
                                <cells>
                                    <ObjectIdentifier title="Boldface Value" text="Column 1 value"/>
                                    <ObjectStatus text="Column 2 value" width="auto"/>
                                    <ObjectStatus text="Column 3 value" width="auto"/>
                                    <ObjectStatus text="Good" width="auto"/>
                                    <ObjectStatus text="Text" width="auto"/>
                                </cells>
                            </ColumnListItem>
                        </items>
                    </Table>
                </items>
            </VBox>
        </f:content>
        <f:footer>
            <OverflowToolbar design="auto">
                <content>
                    <ToolbarSpacer/>
                    <Button text="Save" type="Emphasized" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
                    <Button text="Cancel" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"/>
                </content>
            </OverflowToolbar>
        </f:footer>
    </f:DynamicPage>
</mvc:View>